{
  "name": "admin",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "start": "ng serve --port=4200 --ssl --proxy-config proxy.conf.json",
    "start-dev": "ng serve --port=4200 --ssl --configuration=development --proxy-config proxy.conf.json",
    "start-hml": "ng serve --port=4200 --ssl --configuration=homolog --proxy-config proxy.conf.json",
    "start-prod": "ng serve --port=4200 --ssl --configuration=production --proxy-config proxy.conf.json",
    "prebuild": "npm --no-git-tag-version version patch",
    "build-dev": "ng build --configuration=dev --output-hashing=all",
    "build-hml": "ng build --configuration=hml --output-hashing=all",
    "build": "ng build --configuration=production --output-hashing=all",
    "build-version-17-smaller": "node --openssl-legacy-provider ./node_modules/@angular/cli/bin/ng build"
  },
  "engines": {
    "node": ">=20.10.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.8",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.8",
    "@angular/compiler": "^17.0.8",
    "@angular/core": "^17.0.8",
    "@angular/forms": "^17.0.8",
    "@angular/localize": "^17.0.8",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "^17.0.8",
    "agora-rtc-sdk-ng": "^4.20.0",
    "compressorjs": "^1.2.1",
    "firebase": "^10.7.1",
    "moment": "^2.30.1",
    "ng2-currency-mask": "^13.0.3",
    "ngx-image-cropper": "^7.2.1",
    "ngx-infinite-scroll": "^17.0.0",
    "ngx-mask": "^17.0.4",
    "peerjs": "^1.5.2",
    "rxjs": "~7.8.1",
    "sweetalert2": "^11.10.2",
    "tslib": "^2.3.0",
    "zone.js": "^0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1701.1",
    "@angular-devkit/build-angular": "^17.0.9",
    "@angular/cli": "^17.0.9",
    "@angular/compiler-cli": "^17.0.8",
    "@types/googlemaps": "^3.43.3",
    "@types/jasmine": "~5.1.4",
    "@types/luxon": "^3.4.0",
    "@types/node": "^20.10.8",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.3.3"
  }
}
